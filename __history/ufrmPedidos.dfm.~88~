  object qrBMcidades: TDBISAMQuery
    DatabaseName = 'db'
    EngineVersion = '4.44 Build 3'
    SQL.Strings = (
      'SELECT '
      'A.CODIGO,'
      'A.CEP_INI,'
      'A.CEP_FIM,'
      'A.NOME '
      'FROM CIDADES AS A'
      'WHERE  A.CEP_INI <= :CEP AND :CEP <= A.CEP_FIM ')
    Params = <
      item
        DataType = ftString
        Name = 'CEP'
      end
      item
        DataType = ftString
        Name = 'CEP'
      end>
    Left = 177
    Top = 128
    ParamData = <
      item
        DataType = ftString
        Name = 'CEP'
      end
      item
        DataType = ftString
        Name = 'CEP'
      end>
    object qrBMcidadesCODIGO: TStringField
      FieldName = 'CODIGO'
      Size = 7
    end
    object qrBMcidadesCEP_INI: TStringField
      FieldName = 'CEP_INI'
      Size = 8
    end
    object qrBMcidadesCEP_FIM: TStringField
      FieldName = 'CEP_FIM'
      Size = 8
    end
