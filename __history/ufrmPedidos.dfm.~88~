  object qrBMpedidos: TDBISAMQuery
    DatabaseName = 'db'
    EngineVersion = '4.44 Build 3'
    SQL.Strings = (
      'SELECT '
      ' '#39' '#39' as Enviar,'
      ' A.ID, '
      ' A.JURIDICAFISICA, '
      ' A.NOME, '
      ' A.RG, '
      ' A.CPFCNPJ, '
      ' A.FONE1, '
      ' A.CELULAR, '
      ' A.EMAIL, '
      ' A.LEMBRETE, '
      ' A.FANTASIA, '
      ' A.LIMITECREDITO, '
      ' A.ENDERECO, '
      ' A.CEP, '
      ' A.NUMERO, '
      ' A.COMPLEMENTO, '
      ' A.BAIRRO, '
      ' A.CIDADE, '
      ' A.UF'
      'FROM PESSOAS AS A')
    Params = <>
    Left = 841
    Top = 80
    object qrBMpedidosEnviar: TStringField
      FieldName = 'Enviar'
      Size = 1
    end
