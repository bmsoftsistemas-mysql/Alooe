  object qrBMprodutos: TDBISAMQuery
    DatabaseName = 'db'
    EngineVersion = '4.44 Build 3'
    SQL.Strings = (
      'SELECT '
      'A.ID, '
      'A.PESO_BRUTO, '
      'A.MARCA, '
      'A.SINTEGRA_NCM, '
      'A.DESCRICAO, '
      'A.PRECOVENDA1, '
      'A.PRECOVENDA2,'
      'A.PRECOVENDA4,'
      'A.PRECO_PROMOCAO,'
      'A.PROMOCAO_DATA_INI, '
      'A.PROMOCAO_DATA_FIM, '
      'A.VOLUME_CUBAGEM, '
      'A.ATIVO, '
      'A.DATA_CAD,'
      'A.EXPORTAPALM,'
      'A.EXPORTAINTEGRADOR,'
      'A.COMPRIMENTO,'
      'A.LARGURA,'
      'A.ALTURA,'
      'A.TEXTO,'
      'B.ID as ID_PRODUTOSREFERENCIA,'
      'B.EAN, '
      'B.REFERENCIA, '
      'B.DESCRICAO AS DESCRICAO_VAR,'
      'C.ID_REF,'
      'SUM(C.ESTOQUE) AS ESTOQUE, '
      'C.ESTOQUE_MINIMO, '
      'D.ID as ID_CLASSE, '
      'E.ID_INT, '
      'F.ID_PRODUTO,'
      'F.PRECOCUSTO,'
      'F.CUSTOMEDIO_TOTAL, '
      'C.ID_EMPRESA'
      ''
      'FROM PRODUTOSPRINCIPAL AS A '
      'LEFT JOIN PRODUTOSREFERENCIA AS B ON B.ID_PRODUTO = A.ID'
      'LEFT JOIN PRODUTOSREFEMPRESA AS C ON C.ID_REF = B.ID'
      'LEFT JOIN PRODUTOSCLASSE D ON D.CLASSE = A.CLASSE'
      'LEFT JOIN INTEGRACOES_AUX E ON E.ID_BM = D.ID AND E.TIPO = '#39'CAT'#39
      
        'LEFT JOIN PROCUSTOSEMP F ON F.ID_PRODUTO = A.ID and F.CUSTOMEDIO' +
        '_TOTAL > 0'
      'where A.EXPORTAINTEGRADOR = '#39'S'#39
      ''
      'group by C.ID_REF, F.ID_PRODUTO')
    Params = <>
    Left = 49
    Top = 144
    object qrBMprodutosID: TAutoIncField
      FieldName = 'ID'
    end
    object qrBMprodutosPESO_BRUTO: TFloatField
      FieldName = 'PESO_BRUTO'
    end
    object qrBMprodutosMARCA: TStringField
      FieldName = 'MARCA'
      Size = 15
    end
    object qrBMprodutosSINTEGRA_NCM: TStringField
      FieldName = 'SINTEGRA_NCM'
      Size = 8
    end
    object qrBMprodutosDESCRICAO: TStringField
      FieldName = 'DESCRICAO'
      Size = 80
    end
    object qrBMprodutosPRECOVENDA1: TFloatField
      FieldName = 'PRECOVENDA1'
    end
    object qrBMprodutosPRECOVENDA2: TFloatField
      FieldName = 'PRECOVENDA2'
    end
    object qrBMprodutosPRECOVENDA4: TFloatField
      FieldName = 'PRECOVENDA4'
    end
    object qrBMprodutosPRECO_PROMOCAO: TFloatField
      FieldName = 'PRECO_PROMOCAO'
    end
    object qrBMprodutosPROMOCAO_DATA_INI: TDateField
      FieldName = 'PROMOCAO_DATA_INI'
    end
    object qrBMprodutosPROMOCAO_DATA_FIM: TDateField
      FieldName = 'PROMOCAO_DATA_FIM'
    end
    object qrBMprodutosVOLUME_CUBAGEM: TFloatField
      FieldName = 'VOLUME_CUBAGEM'
    end
    object qrBMprodutosATIVO: TStringField
      FieldName = 'ATIVO'
      Size = 1
    end
    object qrBMprodutosDATA_CAD: TDateField
      FieldName = 'DATA_CAD'
